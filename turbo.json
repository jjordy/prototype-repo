{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "start": {},
    "build": {
      "dependsOn": [
        "db:generate"
      ],
      "outputs": [
        ".next/**",
        "dist/**"
      ]
    },
    "lint": {},
    "dev": {
      "dependsOn": [
        "db:push",
        "db:generate"
      ],
      "cache": false,
      "persistent": true
    },
    "db:generate": {
      "cache": false,
      "dependsOn": [
        "env"
      ]
    },
    "db:push": {
      "cache": false,
      "dependsOn": [
        "env"
      ]
    },
    "env": {
      "cache": false
    }
  }
}